      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "react": "17.0.2",
    "@babel/core": "7.13.15",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "tslib": "2.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "dependencies": {},
  "auto": {
    "plugins": [
      "npm"
    ]
  }
